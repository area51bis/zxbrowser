{
    "programs": [
        {
            "name": "ZXSpin",
            "path": "D:\\emu\\spectrum\\zxspin\\zxspin.exe",
            "args": "${filePath}",
            "ext": [
                "tzx",
                "tap",
                "z80",
                "sna"
            ],
            "default_for": [
                "z80",
                "sna"
            ]
        },
        {
            "name": "ZEsarUX",
            "path": "D:\\emu\\spectrum\\zesarux\\zesarux.exe",
            "args": "--noconfigfile --realloadfast --realtape ${filePath}",
            "ext": [
                "tzx",
                "tap",
                "z80",
                "sna"
            ],
            "unzip": true,
            "default_for": [
                "tzx",
                "tap"
            ]
        },
        {
            "name": "Retro Virtual Machine",
            "path": "D:\\emu\\spectrum\\rvm\\RetroVirtualMachine.exe",
            "args": "-b=zx48k -w -p -c=j\"\"\\n -i ${filePath}",
            "ext": [
                "tzx",
                "tap"
            ],
            "unzip": true
        },
        {
            "name": "Fuse",
            "path": "/Applications/Fuse.app/Contents/MacOS/Fuse",
            "args": "--auto-load --tape ${filePath}",
            "ext": [
                "tzx",
                "tap",
                "z80",
                "sna"
            ],
            "unzip": true
        }
    ],
    "libraries": [
        {
            "type": "zxdb",
            "name": "ZXDB",
            "path": "zxdb"
        }
    ]
}