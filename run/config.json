{
  "programs": [
    {
      "id": "zesarux",
      "name": "ZEsarUX",
      "exec": "/Applications/zesarux.app/Contents/MacOS/zesarux",
      "args": "--realtape '${filePath}'",
      "unzip": true,
      "extensions": ["tzx", "tap"]
    }
  ],

  "default_programs": [
    {
      "extensions": ["tzx", "tap"],
      "program": "zesaurx"
    }
  ]
}
