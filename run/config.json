{
    "programs": [
        {
            "id": "zxspin",
            "name": "ZXSpin",
            "path": "D:\\emu\\spectrum\\zxspin\\zxspin.exe",
            "args": "${filePath}",
            "ext": ["tzx", "tap", "z80"]
        },
        {
            "id": "zesarux",
            "name": "ZEsarUX",
            "path": "D:\\emu\\spectrum\\zesarux\\zesarux.exe",
            "args": "--noconfigfile --realloadfast --realtape ${filePath}",
            "ext": ["tzx", "tap", "z80"],
            "unzip": true
        },
        {
            "id": "fuse",
            "name": "Fuse",
            "path": "/Applications/Fuse.app/Contents/MacOS/Fuse",
            "args": "--auto-load --tape ${filePath}",
            "ext": ["tzx", "tap"],
            "unzip": true
        }
    ],
    "default_programs": [
        {
            "ext": ["z80"],
            "program": "zxspin"
        },
        {
            "ext": ["tzx","tap"],
            "program": "zesarux"
        }
    ],
    "libraries": [
        {
            "type": "zxdb",
            "name": "ZXDB",
            "path": "D:\\downloads\\spectrum\\World of Spectrum"
        },
        {
            "type": "local",
            "name": "Denied",
            "path": "D:\\downloads\\spectrum\\games_moi\\denied"
        }
    ]
}
